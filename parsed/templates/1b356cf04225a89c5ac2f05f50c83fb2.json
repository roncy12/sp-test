{"aoc-custom/components/common/navigation":"<nav class=\"navUser\">\r\n    {{#or customer (unless settings.hide_price_from_guests)}}\r\n        {{> components/common/currency-selector}}\r\n    {{/or}}\r\n\r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n        {{#if customer.store_credit.value '>' 0}}\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action navUser-action--storeCredit\"\r\n                   data-dropdown=\"storeCredit-dropdown\"\r\n                   data-options=\"align:bottom\"\r\n                   href=\"{{urls.cart}}\"\r\n                   aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\r\n                >\r\n                    <span class=\"navUser-action-divider\">\r\n                        {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\r\n                    </span>\r\n                </a>\r\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\r\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        <li class=\"navUser-item\">\r\n            <a class=\"navUser-action navUser-item--compare\"\r\n               href=\"{{urls.compare}}\"\r\n               data-compare-nav\r\n               aria-label=\"{{lang 'common.compare'}}\"\r\n            >\r\n                {{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span>\r\n            </a>\r\n        </li>\r\n        {{#if theme_settings.social_icon_placement_top}}\r\n            <li class=\"navUser-item navUser-item--social\">\r\n                {{> components/common/social-links}}\r\n            </li>\r\n            <li class=\"navUser-item navUser-item--divider\">|</li>\r\n        {{/if}}\r\n        <li class=\"navUser-item\" style=\"display: none;\">\r\n            <button class=\"navUser-action navUser-action--quickSearch\"\r\n               type=\"button\"\r\n               id=\"quick-search-expand\"\r\n               data-search=\"quickSearch\"\r\n               aria-controls=\"quickSearch\"\r\n               aria-label=\"{{lang 'common.search'}}\"\r\n            >\r\n                {{lang 'common.search'}}\r\n            </button>\r\n        </li>\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\"\r\n                   href=\"{{urls.gift_certificate.purchase}}\"\r\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\r\n                >\r\n                    {{lang 'common.gift_cert'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        <li class=\"navUser-item navUser-item--account\">\r\n            {{#if customer}}\r\n                <a class=\"navUser-action\"\r\n                   href=\"{{urls.account.index}}\"\r\n                   aria-label=\"{{lang 'common.account'}}\"\r\n                >\r\n                <img src=\"{{cdn 'https://store-n4601wl9o4.mybigcommerce.com/content/header/icon-accout.svg'}}\" alt=\"header account icon\" class=\"lazyload\">\r\n                    {{lang 'common.account'}}\r\n                </a>\r\n                <a class=\"navUser-action\"\r\n                   href=\"{{urls.auth.logout}}\"\r\n                   aria-label=\"{{lang 'common.logout'}}\"\r\n                   style=\"display: none;\"\r\n                >\r\n                    {{lang 'common.logout'}}\r\n                </a>\r\n            {{else}}\r\n                <a class=\"navUser-action\"\r\n                   href=\"{{urls.auth.login}}\"\r\n                   aria-label=\"{{lang 'common.login'}}\"\r\n                >\r\n                <img src=\"{{cdn 'https://store-n4601wl9o4.mybigcommerce.com/content/header/icon-accout.svg'}}\" alt=\"header account icon\" class=\"lazyload\">\r\n                {{lang 'common.account'}}\r\n                </a>\r\n                {{#if settings.account_creation_enabled}}\r\n                    <span class=\"navUser-or\" style=\"display: none;\">{{lang 'common.or'}}</span>\r\n                    <a class=\"navUser-action\"\r\n                       href=\"{{urls.auth.create_account}}\"\r\n                       aria-label=\"{{lang 'common.sign_up'}}\"\r\n                       style=\"display: none;\"\r\n                    >\r\n                        {{lang 'common.sign_up'}}\r\n                    </a>\r\n                {{/if}}\r\n            {{/if}}\r\n        </li>\r\n        <li class=\"navUser-item navUser-item--cart\">\r\n            <a class=\"navUser-action\"\r\n               data-cart-preview\r\n               data-dropdown=\"cart-preview-dropdown\"\r\n               data-options=\"align:right\"\r\n               href=\"{{urls.cart}}\"\r\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\r\n            >\r\n                <span class=\"navUser-item-cartLabel\" style=\"display: none;\">{{lang 'common.cart'}}</span>\r\n                <img src=\"{{cdn 'https://store-n4601wl9o4.mybigcommerce.com/content/header/icon-cart.svg'}}\" alt=\"cart shipping bag icon\" >\r\n                <span class=\"countPill cart-quantity\"></span>\r\n            </a>\r\n\r\n            <!-- <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div> -->\r\n        </li>\r\n    </ul>\r\n    <p id=\"paragraph_content\">Free domestic shipping on $50 or more <img src=\"{{cdn 'https://store-n4601wl9o4.mybigcommerce.com/content/header/icon-shipping_truck.svg'}}\" alt=\"Shipping truck\"></p>\r\n</nav>\r\n","components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n        >\n            {{lang 'common.currency' code=currency_selector.active_currency_code}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n\n                    {{#if display_name \"===\" \"facebook\"}}\n                    <i class=\"fa-brands fa-facebook-f\"></i>\n                    {{else if display_name \"===\" \"instagram\"}}\n                    <i class=\"fa-brands fa-instagram\"></i>\n                    {{else if display_name \"===\" \"twitter\"}}\n                    <i class=\"fa-brands fa-twitter\"></i>\n                    {{else}}\n                    <svg>\n                        <use href=\"#icon-{{display_name}}\"/>\n                    </svg>\n                    {{/if}}\n                </a>\n            </li>\n        {{/each}}\n        <li class=\"socialLinks-item\">\n            <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                <i class=\"fa-brands fa-tiktok\"></i>\n            </a>\n        </li>\n    </ul>\n{{/if}}\n"}
