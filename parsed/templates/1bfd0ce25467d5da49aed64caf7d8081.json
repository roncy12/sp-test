{"components/common/navigation-list":"{{#if children}}\n<a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   data-collapsible=\"navPages-{{id}}\"\n>\n    {{name}}\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg><use href=\"#icon-chevron-down\" /></svg>\n    </i>\n</a>\n<div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <div class=\"shop_allLink\">\n        <h2>{{name}} <span>Shop All</span>\n            <i class=\"fa fa-angle-right\" aria-hidden=\"true\"></i>\n        </h2>\n        <a href=\"{{url}}\" class=\"to-abso\">{{name}}</a>\n    </div>\n\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item\" style=\"display: none;\">\n            <a class=\"navPage-subMenu-action navPages-action\"\n               href=\"{{url}}\"\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\n            >\n                {{lang 'category.view_all.name' category=name}}\n            </a>\n        </li>\n\n        {{#if id \"===\" \"26\"}}\n            {{#each children}}\n                <li class=\"navPage-subMenu-item\" style=\"{{#if id '==' '34'}}order: 0;{{else if id '==' '36'}} order: 3; {{else if id '==' '35'}} order: 2 {{/if}}\"\">\n                    {{#unless \"===\" children}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                        href=\"{{url}}\"\n                        aria-label=\"{{name}}\"\n                    >\n                        <img src=\"{{cdn (concat 'https://store-n4601wl9o4.mybigcommerce.com/content/sub-categories/' (concat  (lowercase (dashcase name)) '.png'))}}\" alt=\"{{name}}\">\n                        {{#if name \"==\" \"Custom Badge Cards\"}}\n                        <p class=\"category_name\">Custom Badge <br>Cards</p>\n                        {{else if name \"==\" \"Badge Card Sets\" }}\n                        <p class=\"category_name\">Badge Card<br>Sets</p>\n                        {{else if name \"==\" \"Keychain Badge Cards\" }}\n                        <p class=\"category_name\">Keychain <br>Badge Cards</p>\n                        {{else}}\n                        <p class=\"category_name\">{{name}}</p>\n                        {{/if}}\n                    </a>\n                    {{/unless}}\n                </li>\n            {{/each}}\n\n            <li class=\"navPage-subMenu-item\" style=\"order: 1;\">\n                <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                    href=\"{{url}}\"\n                    aria-label=\"{{name}}\"\n                >\n                    <img src=\"{{cdn 'https://store-n4601wl9o4.mybigcommerce.com/content/sub-categories/diy-badge-cards.png'}}\" alt=\"{{name}}\">\n                    <p class=\"category_name\">DIY Badge <br>Cards</p>\n                </a>\n            </li>\n        {{else}}\n        {{#each children}}\n            <li class=\"navPage-subMenu-item\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >\n                        {{name}}\n                        <span class=\"collapsible-icon-wrapper\"\n                            data-collapsible=\"navPages-{{id}}\"\n                            data-collapsible-disabled-breakpoint=\"medium\"\n                            data-collapsible-disabled-state=\"open\"\n                            data-collapsible-enabled-state=\"closed\"\n                        >\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\n                                <svg><use href=\"#icon-chevron-down\" /></svg>\n                            </i>\n                        </span>\n                    </a>\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\n                        {{#each children}}\n                        <li class=\"navPage-childList-item\">\n                            <a class=\"navPage-childList-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                               href=\"{{url}}\"\n                               aria-label=\"{{name}}\"\n                            >\n                                {{name}}\n                            </a>\n                        </li>\n                        {{/each}}\n                    </ul>\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\"\n                       href=\"{{url}}\"\n                       aria-label=\"{{name}}\"\n                    >   \n                        {{#or (if id \"==\" \"26\") (if id \"==\" \"40\")(if id \"==\" \"39\")(if id \"==\" \"38\")}}\n                            <img src=\"{{cdn (concat 'https://store-n4601wl9o4.mybigcommerce.com/content/sub-categories/' (concat  (lowercase (dashcase name)) '.png'))}}\" alt=\"{{name}}\">\n                        {{/or}}\n                        <p class=\"category_name\">{{name}}</p>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n\n        {{/if}}\n    </ul>\n</div>\n{{else}}\n<a class=\"navPages-action{{#if is_active}} activePage{{/if}}\"\n   href=\"{{url}}\"\n   aria-label=\"{{name}}\"\n>\n    {{name}}\n</a>\n{{/if}}\n"}
