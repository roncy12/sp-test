{"components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\n\n                    {{#if display_name \"===\" \"facebook\"}}\n                    <i class=\"fa-brands fa-facebook-f\"></i>\n                    {{else if display_name \"===\" \"instagram\"}}\n                    <i class=\"fa-brands fa-instagram\"></i>\n                    {{else if display_name \"===\" \"twitter\"}}\n                    <i class=\"fa-brands fa-twitter\"></i>\n                    {{else}}\n                    <svg>\n                        <use href=\"#icon-{{display_name}}\"/>\n                    </svg>\n                    {{/if}}\n                </a>\n            </li>\n        {{/each}}\n        <li class=\"socialLinks-item\">\n            <a class=\"icon icon--{{display_name}}\"\n                   href=\"{{url}}\"\n                   target=\"_blank\"\n                   rel=\"noopener\"\n                   title=\"{{{capitalize display_name}}}\"\n                >\n                <i class=\"fa-brands fa-tiktok\"></i>\n            </a>\n        </li>\n    </ul>\n{{/if}}\n"}
