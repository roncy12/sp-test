{"components/faceted-search/faceted-search-navigation":"<div class=\"accordion-nav-clear-holder\">\n    <!-- {{#unless start_collapsed}} is-open{{/unless}} -->\n    <button type=\"button\"\n         class=\"accordion-navigation toggleLink is-open\"\n         data-collapsible=\"#facetedSearch-content--{{dashcase facet}}\"\n         aria-label=\"{{lang 'search.faceted.toggleSection' title=title}}\"\n         aria-expanded=\"true\"\n    >\n        <span class=\"accordion-title\">\n            {{#or (if title \"==\" \"Orientation\")(if title \"==\" \"Durability\")}}  {{title}} <span class=\"title-icon\"><i class=\"far fa-question-circle\"></i><span class=\"tooltip-cat {{title}}\"></span></span> {{else}} {{title}}  {{/or}}\n        </span>\n\n        <span>\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\">\n                <use href=\"#icon-add\" />\n            </svg>\n\n            <span class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\" id=\"categicon\" style=\"display: none;\">\n                <i class=\"fa-solid fa-chevron-down\"></i>\n            </span>\n            \n\n            <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\n                <use href=\"#icon-remove\" />\n            </svg>\n\n            <span class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\" id=\"categicon\" style=\"display: none;\">\n                <i class=\"fa-solid fa-chevron-up\"></i>\n            </span>\n\n        </span>\n    </button>\n    {{#if selected}}\n        <a href=\"{{remove_url}}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\n    {{/if}}\n</div>\n"}
