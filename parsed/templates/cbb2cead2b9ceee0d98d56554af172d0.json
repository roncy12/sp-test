{"components/common/share":"{{#if settings.add_this.buttons}}\n    {{assignVar 'encodedUrl' (encodeURI url)}}\n    {{assignVar 'encodedTitle' (encodeURI head.title)}}\n    <div>\n        <ul class=\"socialLinks\">\n            <li style=\"padding: 0 1rem 0 0;\">Share</li>\n            {{#each settings.add_this.buttons}}\n                <li class=\"socialLinks-item socialLinks-item--{{service}}\" {{#or (if service '==' 'print')(if service '==' 'twitter')}}style='display: none;'{{/or}}>\n                    {{# if service '===' 'print'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       onclick=\"window.print();return false;\"\n                       {{{annotation}}} style=\"display: none;\"\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use href=\"#icon-print\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'twitter'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       href=\"https://twitter.com/intent/tweet/?text={{getVar 'encodedTitle'}}&amp;url={{getVar 'encodedUrl'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       title=\"{{{capitalize service}}}\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <!-- <svg>\n                            <use href=\"#icon-twitter\"/>\n                        </svg> -->\n                        <img src=\"{{cdn '../img/share-icons/icon-twitter.png'}}\" alt=\"twitter share icon\">\n                    </a>\n                    {{else if service '===' 'linkedin'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"https://www.linkedin.com/shareArticle?mini=true&amp;url={{getVar 'encodedUrl'}}&amp;title={{getVar 'encodedTitle'}}&amp;summary={{getVar 'encodedTitle'}}&amp;source={{getVar 'encodedUrl'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <svg>\n                            <use href=\"#icon-linkedin\"/>\n                        </svg>\n                    </a>\n                    {{else if service '===' 'pinterest'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"https://pinterest.com/pin/create/button/?url={{getVar 'encodedUrl'}}&amp;media={{getImageSrcset ../product.main_image (cdn ../theme_settings.default_image_product) 1x=../theme_settings.zoom_size}}&amp;description={{getVar 'encodedTitle'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <!-- <svg>\n                            <use href=\"#icon-pinterest\"/>\n                        </svg> -->\n                        <img src=\"{{cdn '../img/share-icons/icon-pinterest.png'}}\" alt=\"pinterest share icon\">\n                    </a>\n                    {{else if service '===' 'facebook'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n                       target=\"_blank\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <img src=\"{{cdn '../img/share-icons/icon-facebook.png'}}\" alt=\"facebook share icon\">\n                    </a>\n                    {{else if service '===' 'email'}}\n                    <a class=\"socialLinks__link icon icon--{{service}}\"\n                       title=\"{{{capitalize service}}}\"\n                       href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n                       target=\"_self\"\n                       rel=\"noopener\"\n                       {{{annotation}}}\n                    >\n                        <span class=\"aria-description--hidden\">{{{capitalize service}}}</span>\n                        <!-- <svg>\n                            <use href=\"#icon-envelope\"/>\n                        </svg> -->\n                        <img src=\"{{cdn '../img/share-icons/envelope-regular.svg'}}\" alt=\"envelope share icon\">\n                    </a>\n                    {{/if}}\n                </li>\n            {{/each}}\n        </ul>\n    </div>\n{{/if}}\n{{#if settings.facebook_like_button.enabled}}\n    <iframe\n        class=\"facebook{{#if settings.facebook_like_button.verb '===' 'recommend'}}Recommend{{else}}Like{{/if}}Btn\"\n        style=\"border:none; overflow:hidden; margin: 0; padding: 0; position:absolute\"\n        src=\"https://www.facebook.com/plugins/like.php?href={{settings.facebook_like_button.href}}&amp;layout=button_count{{#if settings.facebook_like_button.verb '===' 'recommend'}}&amp;action=recommend{{/if}}&amp;colorscheme=light&amp;height=20\"\n        scrolling=\"no\"\n        frameborder=\"0\"\n        allowTransparency=\"true\">\n    </iframe>\n{{/if}}\n"}
